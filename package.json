{
  "name": "file-server",
  "module": "src/main.ts",
  "type": "module",
  "scripts": {
    "dev": "bun --watch .",
    "start": "bun db:push && ./server",
    "build": "bun build --compile --minify . --outfile server",
    "db:generate": "bun --bun drizzle-kit generate",
    "db:push": "bun --bun drizzle-kit push"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "drizzle-kit": "^0.30.5"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@hono/zod-validator": "^0.4.3",
    "@libsql/client": "^0.15.1",
    "drizzle-orm": "^0.41.0",
    "hono": "^4.7.5",
    "ioredis": "^5.6.0",
    "zod": "^3.24.2"
  }
}
